{
  "manifest_version": 2,

  "name": "Hide Avengers 4 Spoilers",
  "description": "Trying to save you from Avengers End Game Spoiler.",
  "version": "1.0",
  "icons": { 
          "96": "avengers-logo-png.png" },
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["jquery-3.4.0.min.js", "hide-spoiler.js"],
      "run_at": "document_end"
    }
  ]
}
